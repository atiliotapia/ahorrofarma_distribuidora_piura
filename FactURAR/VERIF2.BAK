

SELE A
USE MEDICTDO
SET FILTE TO REGISTRO<100000
SELE B
USE D:\DISTRIBUIDORA\SIGEF31AGO\Sistema_ventas\FACTURAR\MEDICTDO
SET ORDE TO REGISTRO
SELE A
GO TOP
DO WHILE !EOF()
   REG=REGISTRO
   SELE B
   SEEK REG
   IF FOUN()
      TIPOVTA=TIPO_VTA
      CTA=CUENTA_C
 
      SELE A
      REPLA TIPO_VTA WITH TIPOVTA
      REPLA CUENTA_C WITH CTA
   ENDIF
   SELE A
   SKIP
   ?RECN()
ENDDO







PROCEDURE XX
SELE A
USE FACTURA
SELE B
USE D:\DISTRIBUIDORA\SIGEF31AGO\Sistema_ventas\FACTURAR\FACTURA
SET ORDE TO SERFAC
SELE A
GOTO 11
DO WHILE !EOF()
   SER=SERIE
   FAC=NUMERO
   SELE B
   SEEK SER+STR(FAC,8)
   IF FOUN()
      FVC=FECHA_VENC
      DIA=DIAS_CRED
      REG=REGISTRO
      SELE A
      REPLA FECHA_VENC WITH FVC
      REPLA DIAS_CRED WITH DIA
      REPLA REGISTRO WITH REG
   ENDIF
   SELE A
   SKIP
   ?RECN()
ENDDO
